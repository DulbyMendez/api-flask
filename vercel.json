{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "command": "pip install -r requirements.txt && flask build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production"
    }
}